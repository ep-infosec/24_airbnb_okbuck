@args (
String prebuiltType,
String prebuilt,
String source,
String mavenCoords,
boolean enableJetifier,
)
@(ruleType)(
    name = '@name',
    @prebuiltType = '@prebuilt',
@if (valid(source)) {
    source_jar = '@source',
}
@if (valid(mavenCoords)) {
    maven_coords = '@mavenCoords',
}
@if (valid(deps)) {
    deps = [
    @for (dep : sorted(deps)) {
        '@dep',
    }
    ],
}
@if (enableJetifier) {
    enable_jetifier = True,
}
    visibility = ['PUBLIC'],
)
